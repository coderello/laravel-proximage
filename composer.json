{
    "name": "coderello/laravel-proximage",
    "description": "Image proxy for Laravel.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "laravel/framework": "5.5.*|5.6.*|5.7.*|5.8.*|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Coderello\\Proximage\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Coderello\\Proximage\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Coderello\\Proximage\\Providers\\ProximageServiceProvider"
            ]
        }
    }
}
